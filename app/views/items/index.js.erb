$("#page-index-container").ready( function() {
	clearInterval(timer);
	$("#now-loading-bar").css("width", "100%"); 
	$("#now-loading").hide( 1000, function() { 
		$("#page-index-container").show();
	} ); // hide loading
} ); // document.ready

$("#page-index-container").html('<%= escape_javascript(render "items/partials/index") %>');
